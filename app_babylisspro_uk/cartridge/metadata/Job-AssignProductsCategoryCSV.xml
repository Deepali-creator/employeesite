<?xml version="1.0" encoding="UTF-8"?>
<jobs xmlns="http://www.demandware.com/xml/impex/jobs/2015-07-01">
    <job job-id="AssignProductsCategoryCSV" priority="0">
        <description>Converts a CSV information to XML to assign products to multiple categories. Also generates master catalog XML.
CSV structure: category Assignment,Product ID,product_name,description,meta_keywords,meta_description,page_title</description>
        <parameters/>
        <flow>
            <context site-id="uk-babylisspro"/>
            <step step-id="AssignProductsCategoryCSV" type="ExecuteScriptModule" enforce-restart="false">
                <description/>
                <parameters>
                    <parameter name="ExecuteScriptModule.Module">app_babylisspro_uk/cartridge/scripts/impex/AssignProductsCategoryCSV.js</parameter>
                    <parameter name="ExecuteScriptModule.FunctionName">AssignProducts</parameter>
                    <parameter name="ExecuteScriptModule.Transactional">false</parameter>
                    <parameter name="csvFileName">category_finished_goods.csv</parameter>
                    <parameter name="masterCatalogID">master-uk</parameter>
                    <parameter name="storefrontCatalogID">storefront-uk-sparesbabyliss</parameter>
                </parameters>
            </step>
        </flow>
        <rules/>
        <triggers>
            <run-once enabled="false">
                <date>2018-03-07Z</date>
                <time>10:17:32.000Z</time>
            </run-once>
        </triggers>
    </job>

</jobs>
