<?xml version="1.0" encoding="UTF-8" ?>
<?demandware-pipeline version="2.0"?>

<pipeline group="Service">
  <branch basename="_ANONYMOUS_BRANCH_1">
    <segment>
      <node>
        <text-node>
          <description>Renders the customer service overview page.</description>
        </text-node>
        <node-display x="1" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="Show">
    <segment>
      <node>
        <start-node name="Show" secure="false"/>
        <node-display x="1" y="2"/>
      </node>
      <simple-transition/>
      <node>
        <call-node start-name-ref="CustomerService-Links"/>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <interaction-node transaction-required="false">
          <template buffered="true" dynamic="false" name="content/customerservice"/>
        </interaction-node>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_3">
    <segment>
      <node>
        <text-node>
          <description>Renders the left hand navigation.</description>
        </text-node>
        <node-display x="4" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="LeftNav">
    <segment>
      <node>
        <start-node name="LeftNav" secure="false"/>
        <node-display x="4" y="2"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <call-node start-name-ref="CustomerService-Links"/>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <interaction-node transaction-required="false">
          <template buffered="true" dynamic="false" name="content/customerserviceleftnav"/>
        </interaction-node>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_5">
    <segment>
      <node>
        <text-node>
          <description>Fakes and Counterfeits Page</description>
        </text-node>
        <node-display x="8" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="Fakes">
    <segment>
      <node>
        <start-node name="Fakes" secure="false"/>
        <node-display x="8" y="2"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <pipelet-node pipelet-name="ClearFormElement" pipelet-set-identifier="bc_api">
          <key-binding alias="CurrentForms.contactus" key="FormElement"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <transition target-connector="in1" target-path="./+1"/>
    </segment>
    <segment>
      <node>
        <join-node/>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <interaction-continue-node secure="false" start-name="fakescounterfeits" transaction-required="false">
          <template buffered="true" dynamic="false" name="content/fakescounterfeits"/>
        </interaction-continue-node>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="send">
          <transition target-connector="in"/>
          <segment>
            <node>
              <call-node start-name-ref="Recaptcha-Verify"/>
              <node-display x="0" y="1"/>
              <branch basename="b2" source-connector="error">
                <transition target-connector="in1" target-path="..">
                  <transition-display>
                    <bend-point relative-to="source" x="-1" y="0"/>
                    <bend-point relative-to="source" x="-1" y="-2"/>
                  </transition-display>
                </transition>
              </branch>
            </node>
            <simple-transition/>
            <node>
              <pipelet-node pipelet-name="Assign" pipelet-set-identifier="bc_api">
                <config-property key="Transactional" value="false"/>
                <key-binding alias="&quot;edit&quot;" key="From_0"/>
                <key-binding alias="ConfirmationMessage" key="To_0"/>
                <key-binding alias="MailFrom" key="To_1"/>
                <key-binding alias="dw.web.Resource.msg('global.fakescounterfeits.subject','locale',null)" key="From_2"/>
                <key-binding alias="MailSubject" key="To_2"/>
                <key-binding alias="&quot;mail/fakescounterfeitemail&quot;" key="From_3"/>
                <key-binding alias="MailTemplate" key="To_3"/>
                <key-binding alias="dw.system.Site.getCurrent().getCustomPreferenceValue('customerServiceInbox')" key="From_4"/>
                <key-binding alias="MailTo" key="To_4"/>
                <key-binding alias="null" key="From_5"/>
                <key-binding alias="null" key="From_6"/>
                <key-binding alias="null" key="To_6"/>
                <key-binding alias="null" key="From_7"/>
                <key-binding alias="null" key="To_7"/>
                <key-binding alias="null" key="From_8"/>
                <key-binding alias="null" key="To_8"/>
                <key-binding alias="null" key="From_9"/>
                <key-binding alias="null" key="To_9"/>
                <key-binding alias="dw.system.Site.getCurrent().getCustomPreferenceValue('customerServiceEmail')" key="From_1"/>
              </pipelet-node>
              <node-display x="0" y="1"/>
            </node>
            <simple-transition>
              <transition-display>
                <bend-point relative-to="source" x="0" y="1"/>
              </transition-display>
            </simple-transition>
            <node>
              <call-node start-name-ref="Mail-SecureSend"/>
              <node-display x="0" y="1"/>
            </node>
            <transition target-connector="in1" target-path="..">
              <transition-display>
                <bend-point relative-to="source" x="-2" y="-1"/>
                <bend-point relative-to="target" x="-2" y="2"/>
              </transition-display>
            </transition>
          </segment>
        </branch>
      </node>
      <transition target-connector="in1" target-path=".">
        <transition-display>
          <bend-point relative-to="source" x="1" y="-1"/>
        </transition-display>
      </transition>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_7">
    <segment>
      <node>
        <text-node>
          <description>Determines the customer navigation from the folder structure in the content library.</description>
        </text-node>
        <node-display x="1" y="6"/>
      </node>
    </segment>
  </branch>
  <branch basename="Links">
    <segment>
      <node>
        <start-node call-mode="private" name="Links" secure="false"/>
        <node-display x="1" y="7"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <pipelet-node pipelet-name="Script" pipelet-set-identifier="bc_api">
          <config-property key="Transactional" value="false"/>
          <config-property key="OnError" value="PIPELET_ERROR"/>
          <config-property key="ScriptFile" value="customerservice/GetCustomerServiceContent.ds"/>
          <key-binding alias="CustomerServiceLinks" key="CustomerServiceLinks"/>
          <key-binding alias="&quot;customer-service&quot;" key="FolderID"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="error">
          <transition target-connector="in1" target-path="./+1">
            <transition-display>
              <bend-point relative-to="source" x="1" y="0"/>
              <bend-point relative-to="target" x="1" y="0"/>
            </transition-display>
          </transition>
        </branch>
      </node>
      <transition target-connector="in1" target-path="./+1"/>
    </segment>
    <segment>
      <node>
        <join-node/>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <end-node/>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_9">
    <segment>
      <node>
        <text-node>
          <description>Provides a contact us form which sends an email to the configured customer service email address.</description>
        </text-node>
        <node-display x="4" y="6"/>
      </node>
    </segment>
  </branch>
  <branch basename="ContactUs">
    <segment>
      <node>
        <start-node name="ContactUs" secure="true"/>
        <node-display x="4" y="7"/>
      </node>
      <simple-transition/>
      <node>
        <pipelet-node pipelet-name="ClearFormElement" pipelet-set-identifier="bc_api">
          <key-binding alias="CurrentForms.contactus" key="FormElement"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <pipelet-node pipelet-name="UpdatePageMetaData" pipelet-set-identifier="bc_api">
          <key-binding alias="&quot;BaByliss PRO - Contact Us&quot;" key="DefaultTitle"/>
          <key-binding alias="null" key="DefaultKeywords"/>
          <key-binding alias="null" key="DefaultDescription"/>
          <key-binding alias="null" key="Product"/>
          <key-binding alias="null" key="Category"/>
          <key-binding alias="null" key="Content"/>
          <key-binding alias="null" key="Folder"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <transition target-connector="in1" target-path="./+1"/>
    </segment>
    <segment>
      <node>
        <join-node/>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <interaction-continue-node secure="false" start-name="submit" transaction-required="false">
          <template buffered="true" dynamic="false" name="content/contactus"/>
        </interaction-continue-node>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="send">
          <transition target-connector="in"/>
          <segment>
            <node>
              <call-node start-name-ref="Recaptcha-Verify"/>
              <node-display x="0" y="1"/>
              <branch basename="b2" source-connector="error">
                <transition target-connector="in1" target-path="..">
                  <transition-display>
                    <bend-point relative-to="source" x="-1" y="0"/>
                    <bend-point relative-to="source" x="-1" y="-2"/>
                  </transition-display>
                </transition>
              </branch>
            </node>
            <simple-transition>
              <transition-display>
                <bend-point relative-to="source" x="0" y="1"/>
              </transition-display>
            </simple-transition>
            <node>
              <call-node start-name-ref="CustomerService-MatchEmail"/>
              <node-display x="0" y="1"/>
              <branch basename="b3" source-connector="error">
                <transition target-connector="in1" target-path="..">
                  <transition-display>
                    <bend-point relative-to="source" x="-2" y="-1"/>
                    <bend-point relative-to="source" x="-2" y="-2"/>
                    <bend-point relative-to="target" x="-2" y="1"/>
                  </transition-display>
                </transition>
              </branch>
            </node>
            <simple-transition/>
            <node>
              <pipelet-node pipelet-name="Assign" pipelet-set-identifier="bc_api">
                <config-property key="Transactional" value="false"/>
                <key-binding alias="&quot;edit&quot;" key="From_0"/>
                <key-binding alias="ConfirmationMessage" key="To_0"/>
                <key-binding alias="MailFrom" key="To_1"/>
                <key-binding alias="CurrentForms.contactus.myquestion.value" key="From_2"/>
                <key-binding alias="MailSubject" key="To_2"/>
                <key-binding alias="&quot;mail/contactus&quot;" key="From_3"/>
                <key-binding alias="MailTemplate" key="To_3"/>
                <key-binding alias="dw.system.Site.getCurrent().getCustomPreferenceValue('customerServiceInbox')" key="From_4"/>
                <key-binding alias="MailTo" key="To_4"/>
                <key-binding alias="null" key="From_5"/>
                <key-binding alias="null" key="To_5"/>
                <key-binding alias="null" key="From_6"/>
                <key-binding alias="null" key="To_6"/>
                <key-binding alias="null" key="From_7"/>
                <key-binding alias="null" key="To_7"/>
                <key-binding alias="null" key="From_8"/>
                <key-binding alias="null" key="To_8"/>
                <key-binding alias="null" key="From_9"/>
                <key-binding alias="null" key="To_9"/>
                <key-binding alias="CurrentForms.contactus.email.value" key="From_1"/>
              </pipelet-node>
              <node-display x="0" y="1"/>
            </node>
            <simple-transition>
              <transition-display>
                <bend-point relative-to="source" x="0" y="1"/>
              </transition-display>
            </simple-transition>
            <node>
              <call-node start-name-ref="Mail-SecureSend"/>
              <node-display x="0" y="1"/>
            </node>
            <transition target-connector="in1" target-path="..">
              <transition-display>
                <bend-point relative-to="source" x="-3" y="1"/>
                <bend-point relative-to="target" x="-3" y="0"/>
              </transition-display>
            </transition>
          </segment>
        </branch>
      </node>
      <transition target-connector="in1" target-path=".">
        <transition-display>
          <bend-point relative-to="source" x="1" y="-1"/>
        </transition-display>
      </transition>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_11">
    <segment>
      <node>
        <text-node>
          <description>Change the MailTo in order to send to the store's customer service email address.  It defaults to the user's email for demonstration.</description>
        </text-node>
        <node-display x="9" y="7"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_12">
    <segment>
      <node>
        <text-node>
          <description>To get the list of customer service folders we are setting the FolderID parameter to &quot;customer-service&quot;.  This is the root folder for customer service content</description>
        </text-node>
        <node-display height="2" x="0" y="8"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_13">
    <segment>
      <node>
        <text-node>
          <description>Check if Email ID is matching with Confirm Email ID</description>
        </text-node>
        <node-display x="8" y="10"/>
      </node>
    </segment>
  </branch>
  <branch basename="MatchEmail">
    <segment>
      <node>
        <start-node call-mode="private" name="MatchEmail" secure="false"/>
        <node-display x="8" y="11"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <decision-node condition-key="CurrentForms.contactus.email.value.equals(CurrentForms.contactus.emailconfirm.value)" condition-operator="expr"/>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="yes">
          <transition target-connector="in">
            <transition-display>
              <bend-point relative-to="source" x="0" y="1"/>
            </transition-display>
          </transition>
          <segment>
            <node>
              <end-node/>
              <node-display x="0" y="1"/>
            </node>
          </segment>
        </branch>
      </node>
      <simple-transition/>
      <node>
        <pipelet-node pipelet-name="InvalidateFormElement" pipelet-set-identifier="bc_api">
          <key-binding alias="CurrentForms.contactus.emailconfirm" key="FormElement"/>
        </pipelet-node>
        <node-display orientation="horizontal" x="1" y="0"/>
      </node>
      <simple-transition/>
      <node>
        <end-node name="error"/>
        <node-display orientation="horizontal" x="1" y="0"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_15">
    <segment>
      <node>
        <text-node>
          <description>Change the MailTo in order to send to the store's customer service email address.  It defaults to the user's email for demonstration.</description>
        </text-node>
        <node-display x="5" y="12"/>
      </node>
    </segment>
  </branch>
</pipeline>
